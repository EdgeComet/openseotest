# openseotest.org Environment Configuration
#
# Copy this file to .env and update the values for your environment.
# The .env file should never be committed to version control.

# Application Environment
# Options: development, production, testing
APP_ENV=development

# Debug Mode
# Set to 'false' in production to hide error details
APP_DEBUG=true

# Application URL
# Used for generating absolute URLs (sitemap, etc.)
APP_URL=http://localhost:8000

# Timezone
# Default: UTC
APP_TIMEZONE=UTC
